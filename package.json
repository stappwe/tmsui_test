{
  "name": "TMS",
  "version": "10.0.0",
  "license": "EUPL-1.1",
  "scripts": {
    "ng": "ng",
    "e2e-generator": "npx playwright codegen localhost:4207/#/tms/events/list",
    "e2e-simple": "playwright test --project=chromium",
    "e2e-all-parallel": "playwright test --workers 3",
    "e2e-all": "playwright test",
    "e2e-report": "npx playwright show-report playwright-report",
    "postinstall:tms": "tslint --project ./projects/tms/tsconfig.app.json --fix",
    "postinstall": "npm run postinstall:tms",
    "stylelint": "stylelint \"src/**/*.scss\"",
    "tslint": "tslint \"src/**/*.ts\"",
    "clean": "rimraf out-tsc dist packages/**/dist/** packages/**/out-tsc/** packages/**/src/lib/**/*.js packages/**/src/lib/**/*.js.map",
    "clean-extra": "rimraf dist-library",
    "setup": "npm install && npm run clean && npm run clean-extra && npm run build:lib",
    "start-mock-server-mean": "nodemon --ext js --watch ./mock/mean ./mock/mean/server.js",
    "start-mock-server": "nodemon --ext js --watch ./mock/json ./mock/json/server.js",
    "start-serve:tms:vm": "eui-scripts serve-app --project=tms --configuration=proxy-mock --vendor-source-map",
    "start:tms:vm": "npm run clean && npm-run-all --parallel start-mock-server start-serve:tms:vm",
    "start-proxy:tms": "npm run clean && eui-scripts serve-app --project=tms --configuration=proxy",
    "start-openid:tms": "npm run clean && eui-scripts serve-app --project=tms --configuration=local-openid",
    "build:lib": "ng build tms-library",
    "pkg:init": "eui-cli -m",
    "pkg:clean": "eui-scripts clean-package",
    "pkg:clean-all": "eui-scripts clean-all",
    "pkg:test": "eui-scripts test-package",
    "pkg:build": "eui-scripts build-package",
    "pkg:build-deps": "eui-scripts build-deps",
    "pkg:build-all": "eui-scripts build-all",
    "pkg:build-element": "eui-scripts build-element",
    "build-tms-prod:all": "npm run pkg:build-all && npm run build-tms",
    "pkg:publish": "eui-scripts publish",
    "pkg:publish-all": "eui-scripts publish-all",
    "pkg:publish-all:citnet": "npm run publish-all -- --citnet",
    "pkg:publish-all:verdaccio": "npm run publish-all -- --verdaccio",
    "generate-changelog": "eui-scripts generate-changelog",
    "test_component": "jest --config jest.lib.config.js request-error-interceptior.service.spec.ts",
    "test:component": "jest --config jest.lib.config.js",
    "test:lib": "jest --config jest.lib.config.js --collectCoverage",
    "test:tms": "jest --config ./jest.tms.config.js"
  },
  "private": true,
  "dependencies": {
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "10.2.5",
    "@angular/pwa": "^0.1002.3",
    "@eui/deps-base": "10.4.7",
    "ag-grid-angular": "25.3.0",
    "ag-grid-community": "25.3.0",
    "ag-grid-enterprise": "25.3.0",
    "esri-leaflet": "^2.5.0",
    "immutable": "^3.8.2",
    "jwt-decode": "^3.1.2",
    "leaflet": "^1.7.1",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "playwright": "^1.18.0",
    "shaka-player": "^3.2.0",
    "xlsx": "^0.17.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.17.1",
    "@types/core-js": "^2.5.5",
    "@types/jest": "^26.0.24",
    "@types/moment-timezone": "0.5.30",
    "jest": "^26.5.3",
    "jest-preset-angular": "^8.4.0"
  }
}
